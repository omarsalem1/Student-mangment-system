@model department
@{
    ViewBag.Title = "create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>create</h2>

@using (Html.BeginForm("create", "department", FormMethod.Post))
{
    @Html.LabelFor(a => a.dept_id);
    @Html.EditorFor(a => a.dept_id);
    @Html.ValidationMessageFor(a => a.dept_id);
    @Html.LabelFor(a => a.dept_name);
    @Html.EditorFor(a => a.dept_name);
    @Html.ValidationMessageFor(a => a.dept_name);
    <br />
    <input type="submit" value="add department" class="btn-success" />
}
@section scripts{
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
}

